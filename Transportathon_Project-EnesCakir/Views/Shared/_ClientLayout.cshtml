<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Transportathon_Project_EnesCakir</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Transportathon_Project_EnesCakir.styles.css" asp-append-version="true" />
</head>
<body>
    <div class="container w-75 pt-5">
        <div class="row">
            <div class="col-2">
                @await Html.PartialAsync("./PartialViews/_SideNavigationPartialView")
            </div>
            <div class="col-10">
                @RenderBody()
            </div>
        </div>
    </div>
    <div class="container">
        <main role="main" class="pb-3">
            
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2023 - Transportathon_Project_EnesCakir - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
